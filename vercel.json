{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "index.js"
    },
    {
      "src": "/reset",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}